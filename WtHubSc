local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Water Hub (Unworking)", "Ocean")

wait(0.2)
game.StarterGui:SetCore("SendNotification", {
Title = "Water Hub <->"; 
Text = "E̶̳͂̊̌̿̓͒̉͒̍̕r̶̡̈́͛ṛ̴̢̛̝͍̳̰͉͒̄͋̇̒͐̋̏ơ̸̢̧͇̱͇̗̰̣͎̋͋̀̅͌̕̚͠ŗ̷̛͕̬̒́̂͜"; 
Duration = 5; 
})

--Start

local Tab = Window:NewTab("Start")
local Section = Tab:NewSection("Info")

Section:NewLabel("New Roblox Hack")
Section:NewLabel("Owner : SuperMan_VN")
Section:NewLabel("Water Hub Status: N/A")
Section:NewLabel("I'm actively working to fix this")
--Main
local main = Window:NewTab("Main")
local main = main:NewSection("Some simple hack here")

main:NewToggle("Super Human", "Go Fast And Jump High", function(state)
    if state then
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = 50
    else
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = 50
    end
end)

main:NewToggle("Speed Hack", "Click to go faster", function(state)
    if state then
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
    else
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
    end
end)

main:NewButton("Press X to fly", "No Desc", function()
    print("unworked now!")
end)

--Player
local plr = Window:NewTab("Local player")
local plr = plr:NewSection("Player")

plr:NewSlider("Custom WP", "No Desc", 16, 16, function(s)
    game.Players.LocalPlayer.Character.Humanoid.nth = s
end)

plr:NewSlider("Custom JP", "No Desc", 50, 50, function(s) 
    game.Players.LocalPlayer.Character.Humanoid.nth = s
end)

--Fun Cheat
local fun = Window:NewTab("Fun Cheat")
local fun = fun:NewSection("Hmmmm...")

fun:NewButton("Reset Character", "Oof", function()
    print("Unworked now!")
end)

fun:NewToggle("Over Heaven", "No Desc", function(state)
    if state then
        game.Players.LocalPlayer.Character.Humanoid.HipHeight = 100404
    else
        game.Players.LocalPlayer.Character.Humanoid.HipHeight = 2
    end
end)

fun:NewButton("Over Heaven 2", "Breh", function()
    game.Players.LocalPlayer.Character.Humanoid.Health = 0
end)

fun:NewButton("Weak Human", "No Desc", function()
    game.Players.LocalPlayer.Character.Humanoid.Health = 50
end)

fun:NewToggle("Super Human 2", "No Desc", function(state)
    if state then
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
		game.Players.LocalPlayer.Character.Humanoid.JumpPower = 50
    else
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
		game.Players.LocalPlayer.Character.Humanoid.JumpPower = 50
    end
end)

--hack
local hac = Window:NewTab("Other Hack")
local hac = hac:NewSection("More Hack here!")

hac:NewButton("Infinitie Yield", "Last button working now", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
end)

hac:NewLabel("Other Hack Comming Soon!")

